                                                     ImageSource="Images\redirect.png" />
                        <r:RibbonSeparator />
                        <r:RibbonApplicationMenuItem x:Name="rapmnitSalir" Click="bttSalir_Click"
                                                     Header="Salir" 
                                                     ImageSource="Images\exit.png" />
                    </r:RibbonApplicationMenu>
                </r:Ribbon.ApplicationMenu>

                <!--HelpPaneContent-->
                <r:Ribbon.HelpPaneContent>
                    <r:RibbonButton x:Name="rbttHelp" 
                                    SmallImageSource="Images\help.png" 
                                    Click="bttHelp_Click" 
                                    ToolTipTitle="Ayuda"
                                    ToolTipDescription="Aquí ponemos algún texto..."
                                    ToolTipFooterTitle="Más información" 
                                    ToolTipFooterDescription="Pulse F1 para más ayuda..." 
                                    ToolTipService.ShowDuration="10000"/>
                </r:Ribbon.HelpPaneContent>
            
