<r:RibbonWindow x:Class="KRibbon.View.MainWindow"    
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
                xmlns:r="clr-namespace:Microsoft.Windows.Controls.Ribbon;assembly=RibbonControlsLibrary"
                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"                
                xmlns:local="clr-namespace:KRibbon"
                xmlns:vm="clr-namespace:KRibbon.ViewModel.GenericViewModel"                
                xmlns:sys="clr-namespace:System;assembly=mscorlib"
                xmlns:uc="clr-namespace:KRibbon.View"
                mc:Ignorable="d"
                Title="MainWindow" x:Name="mainwindow" WindowState="Maximized" Closing="mainwindow_Closing">
    <r:RibbonWindow.DataContext>
        <vm:SetLanguagesViewModel/>
    </r:RibbonWindow.DataContext>
    <DockPanel x:Name="RibbonWindow">
        <!--StatusBar-->
        <StatusBar DockPanel.Dock="Bottom" x:Name="stbrInicio">
            <StatusBarItem>
                <TextBlock Name="stInicio" />
            </StatusBarItem>
        </StatusBar>
        <!--Permite hacer scroll entre las diferentes tabs de la ribbon-->
        <!--<ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Hidden">-->
        <r:Ribbon x:Name="rbInicio" DockPanel.Dock="Top" Title="Aquí ponemos algún texto, p.e.: KarveWin [Versión: 01/01/2000 00:00]         C1[PRUEBA, S.A]. USUARIO: JORDI">
            <r:Ribbon.Resources>
                <!--Resource que permite hacer drag&drop entre los ribbongroups-->
                <Style TargetType="r:RibbonGroup">
                    <Setter Property="AllowDrop" Value="True"/>
                    <EventSetter Event="PreviewMouseMove" Handler="RibbonGroup_PreviewMouseMove"/>
                    <EventSetter Event="Drop" Handler="RibbonGroup_Drop"/>
                </Style>

                <!--Resource que añade un ContextMenu personalizado a los items que deseemos-->
                <!--<r:RibbonContextMenu x:Key="AnadirAFavoritos" x:Name="rctmnAnadir">
                        <r:RibbonMenuItem Header="Añadir a Favoritos" x:Name="rctmnitSalir" >
                            <r:RibbonMenuItem.Icon>
                                <Image Source="..\Images\new.png" />
                            </r:RibbonMenuItem.Icon>
                        </r:RibbonMenuItem>
                        <Separator/>
                        <r:RibbonMenuItem Header="Item1" >
                            <r:RibbonMenuItem.Icon>
                                <Image Source="..\Images\cut.png" />
                            </r:RibbonMenuItem.Icon>
                        </r:RibbonMenuItem>
                        <r:RibbonMenuItem Header="Item2" Command="Copy">
                            <r:RibbonMenuItem.Icon>
                                <Image Source="..\Images\copy.png" />
                            </r:RibbonMenuItem.Icon>
                        </r:RibbonMenuItem>
                        <r:RibbonMenuItem Header="Item3" Command="Paste">
                            <r:RibbonMenuItem.Icon>
                                <Image Source="..\Images\paste.png" />
                            </r:RibbonMenuItem.Icon>
                        </r:RibbonMenuItem>
                    </r:RibbonContextMenu>-->
                <!--Resource que da formato a los RibbonGroup cuando se minimiza/collapse la pantalla-->
                <r:RibbonGroupSizeDefinitionBaseCollection x:Key="RibbonGroupLayoutLarge">
                    <r:RibbonGroupSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Large" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Large" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Large" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Large" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Large" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Large" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Large" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Large" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                    </r:RibbonGroupSizeDefinition>
                    <r:RibbonGroupSizeDefinition IsCollapsed="True"></r:RibbonGroupSizeDefinition>
                </r:RibbonGroupSizeDefinitionBaseCollection>
                <r:RibbonGroupSizeDefinitionBaseCollection x:Key="RibbonGroupLayoutSmall">
                    <r:RibbonGroupSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Small" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Small" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Small" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Small" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Small" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Small" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Small" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                    </r:RibbonGroupSizeDefinition>
                    <r:RibbonGroupSizeDefinition IsCollapsed="True"></r:RibbonGroupSizeDefinition>
                </r:RibbonGroupSizeDefinitionBaseCollection>
            </r:Ribbon.Resources>

            <!--ContextualTabGroups-->
            <r:Ribbon.ContextualTabGroups>
                <r:RibbonContextualTabGroup Header="Karve" Visibility="Visible" Background="Coral" />
                <r:RibbonContextualTabGroup Header="Acciones" Visibility="Visible" Background="Azure" />
            </r:Ribbon.ContextualTabGroups>

            <!--QuickAccessToolBar-->
            <r:Ribbon.QuickAccessToolBar>
                <r:RibbonQuickAccessToolBar Visibility="Visible" >
                    <r:RibbonButton x:Name="rbtntCortar" 
                                    Label="Cortar" 
                                    SmallImageSource="..\Images\copy.png" 
                                    Command="Cut" />
                    <r:RibbonButton x:Name="rbtntCopiar" 
                                    Label="Copiar" 
                                    SmallImageSource="..\Images\copy.png" 
                                    Command="Copy" />
                    <r:RibbonButton x:Name="rbtntPegar" 
                                    Label="Pegar" 
                                    SmallImageSource="..\Images\paste.png" 
                                    Command="Paste" />
                    <r:RibbonButton x:Name="rbtnEliminar" 
                                    Label="Eliminar" 
                                    SmallImageSource="..\Images\delete.png" 
                                    Command="Delete" />
                    <r:RibbonSplitButton x:Name="rbtnRehacer" 
                                         Label="Rehacer" 
                                         SmallImageSource="..\Images\redo.png" 
                                         Command="Redo" >
                        <r:RibbonSplitMenuItem Header="Redo 1" />
                        <r:RibbonSplitMenuItem Header="Redo 2" />
                    </r:RibbonSplitButton>
                    <r:RibbonSplitButton x:Name="rbtntDeshacer" 
                                         Label="Deshacer" 
                                         SmallImageSource="..\Images\undo.png" 
                                         Command="Undo" >
                        <r:RibbonSplitMenuItem Header="Undo 1" />
                        <r:RibbonSplitMenuItem Header="Undo 2" />
                        <r:RibbonSplitMenuItem Header="Undo 3" />
                    </r:RibbonSplitButton>
                    <r:RibbonButton x:Name="rbtntOpciones" 
                                    Label="Opciones" 
                                    SmallImageSource="..\Images\tools.png" />
                </r:RibbonQuickAccessToolBar>
            </r:Ribbon.QuickAccessToolBar>

            <!--AplicationMenu-->
            <r:Ribbon.ApplicationMenu >
                <r:RibbonApplicationMenu x:Name="rapmnInicio"  SmallImageSource="..\Images\home.png" Label="{Binding lrapmnInicio, Source={StaticResource ResourceLanguage}}" KeyTip="H">
                    <r:RibbonApplicationMenuItem x:Name="rapmnitAbrir" 
                                                 Header="{Binding lrapmnitAbrir, Source={StaticResource ResourceLanguage}}"
                                                 ImageSource="..\Images\open.png" 
                                                 Command="Open" />
                    <r:RibbonApplicationMenuItem x:Name="rapmnitGuardar" 
                                                 Header="{Binding lrapmnitGuardar, Source={StaticResource ResourceLanguage}}" 
                                                 ImageSource="..\Images\save.png" 
                                                 Command="Save" />
                    <r:RibbonSeparator />
                    <r:RibbonApplicationMenuItem x:Name="rapmnitRelogin" 
                                                 Header="{Binding lrapmnitRelogin, Source={StaticResource ResourceLanguage}}" 
                                                 ImageSource="..\Images\login.png" />
                    <r:RibbonApplicationMenuItem x:Name="rapmnitReconectar" 
                                                 Header="{Binding lrapmnitReconectar, Source={StaticResource ResourceLanguage}}" 
                                                 ImageSource="..\Images\conect.png" />
                    <r:RibbonSeparator />
                    <r:RibbonApplicationMenuItem x:Name="rapmnitIrAlquiler" 
                                                 Header="{Binding lrapmnitIrAlquiler, Source={StaticResource ResourceLanguage}}"
                                                 ImageSource="..\Images\redirect.png" />
                    <r:RibbonApplicationMenuItem x:Name="rapmnitIrContabilidad" 
                                                 Header="{Binding lrapmnitIrContabilidad, Source={StaticResource ResourceLanguage}}" 
                                                 ImageSource="..\Images\redirect.png" />
                    <r:RibbonApplicationMenuItem x:Name="rapmnitIrTaller" 
                                                 Header="{Binding lrapmnitIrTaller, Source={StaticResource ResourceLanguage}}"
                                                 ImageSource="..\Images\redirect.png" />
                    <r:RibbonSeparator />
                    <r:RibbonApplicationMenuItem x:Name="rapmnitSalir"
                                                 Header="{Binding lrapmnitSalir, Source={StaticResource ResourceLanguage}}"
                                                 ImageSource="..\Images\exit.png"                                     
                                                 DataContext="{StaticResource closeWindowViewModel}" >
                        <r:RibbonApplicationMenuItem.InputBindings>
                            <KeyBinding Key="Return"                                                
                                        Command="{Binding CloseWindowCommand}" 
                                        CommandParameter="Window" />
                            <MouseBinding MouseAction="LeftClick"
                                          Command="{Binding CloseWindowCommand}" 
                                          CommandParameter="Window" />
                        </r:RibbonApplicationMenuItem.InputBindings>
                    </r:RibbonApplicationMenuItem>
                </r:RibbonApplicationMenu>
            </r:Ribbon.ApplicationMenu>

            <!--HelpPaneContent-->
            <r:Ribbon.HelpPaneContent>
                <r:RibbonButton x:Name="rbtnHelp" 
                                SmallImageSource="..\Images\help.png" 
                                Click="btnHelp_Click" 
                                ToolTipTitle="{Binding lrbtnHelpTitle, Source={StaticResource ResourceLanguage}}"
                                ToolTipDescription="{Binding lrbtnHelpDescr, Source={StaticResource ResourceLanguage}}"
                                ToolTipFooterTitle="{Binding lrbtnHelpFooterTitle, Source={StaticResource ResourceLanguage}}" 
                                ToolTipFooterDescription="{Binding lrbtnHelpFooterDescr, Source={StaticResource ResourceLanguage}}" 
                                ToolTipService.ShowDuration="10000"/>
            </r:Ribbon.HelpPaneContent>

            <!--Tab Maestros-->
            <r:RibbonTab x:Name="rbtbMaestros"
                         Header="{Binding lrbtbMaestros, Source={StaticResource ResourceLanguage}}"
                         KeyTip="M"
                         GroupSizeReductionOrder="rgrAuxiliares, rgrCentrosAlquiler, rgrClientes">
                <r:RibbonGroup x:Name="rgrCentrosAlquiler" Header="{Binding lrrCentrosAlquiler, Source={StaticResource ResourceLanguage}}" LargeImageSource="..\Images\open.png" >
                    <r:RibbonButton x:Name="rbtnEmpresas" 
                                    Label="{Binding lrbtnEmpresas, Source={StaticResource ResourceLanguage}}"
                                    SmallImageSource="..\Images\new.png" >
                    </r:RibbonButton>
                    <r:RibbonButton x:Name="rbtnOficinas"
                                    Label="{Binding lrbtnOficinas, Source={StaticResource ResourceLanguage}}"
                                    SmallImageSource="..\Images\user.png" >
                    </r:RibbonButton>
                    <r:RibbonButton x:Name="rbtnZonasOficina"
                                    Label="{Binding lrbtnZonasOficina, Source={StaticResource ResourceLanguage}}"
                                    SmallImageSource="..\Images\group.png" >
                    </r:RibbonButton>
                </r:RibbonGroup>
                <r:RibbonGroup x:Name="rgrClientes" Header="{Binding lrgrClientes, Source={StaticResource ResourceLanguage}}" LargeImageSource="..\Images\open.png">
                    <r:RibbonButton x:Name="rbtnClientes" 
                                    Label="{Binding lrbtnClientes, Source={StaticResource ResourceLanguage}}" 
                                    SmallImageSource="..\Images\new.png">
                    </r:RibbonButton>
                    <r:RibbonButton x:Name="rbtnCambioCodigoCliente" 
                                    Label="{Binding lrbtnCambioCodigoCliente, Source={StaticResource ResourceLanguage}}" 
                                    SmallImageSource="..\Images\new.png" >
                    </r:RibbonButton>
                    <r:RibbonButton x:Name="rbtnEntradaRapida" 
                                    Label="{Binding lrbtnEntradaRapida, Source={StaticResource ResourceLanguage}}" 
                                    SmallImageSource="..\Images\new.png" >
                    </r:RibbonButton>
                </r:RibbonGroup>
                <r:RibbonGroup x:Name="rgrComisionistas" Header="{Binding lrgrComisionistas, Source={StaticResource ResourceLanguage}}" LargeImageSource="..\Images\open.png">
                    <r:RibbonButton x:Name="rbtnComisionistas"
                                    Label="{Binding lrbtnComisionistas, Source={StaticResource ResourceLanguage}}"
                                    SmallImageSource="..\Images\new.png"
                                    LargeImageSource="..\Images\new.png" >
                        
                    </r:RibbonButton>
                </r:RibbonGroup>
                <r:RibbonGroup x:Name="rgrProveedores" Header="{Binding lrgrProveedores, Source={StaticResource ResourceLanguage}}" LargeImageSource="..\Images\open.png">
                    <r:RibbonButton x:Name="rbtnProveedores"
                                    Label="{Binding lrbtnProveedores, Source={StaticResource ResourceLanguage}}"
                                    SmallImageSource="..\Images\new.png"
                                    LargeImageSource="..\Images\new.png" >
                    </r:RibbonButton>
                </r:RibbonGroup>
                <r:RibbonGroup x:Name="rgrTarifas" Header="{Binding lrgrTarifas, Source={StaticResource ResourceLanguage}}" LargeImageSource="..\Images\open.png">
                    <r:RibbonMenuButton x:Name="rmnbtnTarifas" Label="{Binding lrmnbtnTarifas, Source={StaticResource ResourceLanguage}}" LargeImageSource="..\Images\open.png" KeyTip="T" >
                        <r:RibbonButton x:Name="rbtnTarifas" 
                                        Label="{Binding lrbtnTarifas, Source={StaticResource ResourceLanguage}}" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnCambioCodigoTarifas" 
                                        Label="{Binding lrbtnCambioCodigoTarifas, Source={StaticResource ResourceLanguage}}" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnCrearTarifaDesdeOtraTarifa" 
                                        Label="{Binding lrbtnCrearTarifaDesdeOtraTarifa, Source={StaticResource ResourceLanguage}}" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnIncrementoPrecios" 
                                        Label="{Binding lrbtnIncrementoPrecios, Source={StaticResource ResourceLanguage}}" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnTarifaCliente" 
                                        Label="{Binding lrbtnTarifaCliente, Source={StaticResource ResourceLanguage}}" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnTarifasComisionista" 
                                            Label="{Binding lrbtnTarifasComisionista, Source={StaticResource ResourceLanguage}}" 
                                            SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnVerTarifa" 
                                        Label="{Binding lrbtnVerTarifa, Source={StaticResource ResourceLanguage}}" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnVincularClienteTarifa" 
                                        Label="{Binding lrbtnVincularClienteTarifa, Source={StaticResource ResourceLanguage}}" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                    </r:RibbonMenuButton>
                </r:RibbonGroup>
                <r:RibbonGroup x:Name="rgrVehiculos" Header="{Binding lrgrVehiculos, Source={StaticResource ResourceLanguage}}" LargeImageSource="..\Images\open.png">
                    <r:RibbonMenuButton x:Name="rmnbtnVehiculos" Label="{Binding lrmnbtnVehiculos, Source={StaticResource ResourceLanguage}}" LargeImageSource="..\Images\open.png" KeyTip="V">
                        <r:RibbonButton x:Name="rbtnVehiculos" 
                                        Label="{Binding lrbtnVehiculos, Source={StaticResource ResourceLanguage}}" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnCambioCodigoVehiculo" 
                                        Label="{Binding lrbtnCambioCodigoVehiculo, Source={StaticResource ResourceLanguage}}" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnModificacionMasivaAux" 
                                        Label="{Binding lrbtnModificacionMasivaAux, Source={StaticResource ResourceLanguage}}" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnRecalculoMantenimiento" 
                                        Label="{Binding lrbtnRecalculoMantenimiento, Source={StaticResource ResourceLanguage}}" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                    </r:RibbonMenuButton>
                </r:RibbonGroup>
                <r:RibbonGroup x:Name="rgrAuxiliares" Header="{Binding lrgrAuxiliares, Source={StaticResource ResourceLanguage}}" LargeImageSource="..\Images\open.png" GroupSizeDefinitions="{StaticResource RibbonGroupLayoutLarge}">
                    <r:RibbonMenuButton x:Name="rmnbtnAuxiliaresClientes" Label="{Binding lrmnbtnAuxiliaresClientes, Source={StaticResource ResourceLanguage}}" LargeImageSource="..\Images\open.png" KeyTip="C" >
                        <r:RibbonButton x:Name="rbtnBancosClientes" 
                                        Label="{Binding lrbtnBancosClientes, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png"
                                        DataContext="{StaticResource mostrarAuxiliaresViewModel}" >
                            <r:RibbonButton.InputBindings>
                                <KeyBinding Key="Return"                                                
                                            Command="{Binding MostrarAuxCommand}" 
                                            CommandParameter="{Binding RelativeSource={RelativeSource AncestorType={x:Type r:RibbonButton}}, Path=Name}" />
                                <MouseBinding MouseAction="LeftClick"
                                              Command="{Binding MostrarAuxCommand}" 
                                              CommandParameter="{Binding RelativeSource={RelativeSource AncestorType={x:Type r:RibbonButton}}, Path=Name}" />
                            </r:RibbonButton.InputBindings>
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnBloqueFacturacion" 
                                        Label="{Binding lrbtnBloqueFacturacion, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" 
                                        DataContext="{StaticResource mostrarAuxiliaresViewModel}" >
                            <r:RibbonButton.InputBindings>
                                <KeyBinding Key="Return"                                                
                                            Command="{Binding MostrarAuxCommand}"
                                            CommandParameter="{Binding RelativeSource={RelativeSource AncestorType={x:Type r:RibbonButton}}, Path=Name}" />
                                <MouseBinding MouseAction="LeftClick"
                                              Command="{Binding MostrarAuxCommand}" 
                                              CommandParameter="{Binding RelativeSource={RelativeSource AncestorType={x:Type r:RibbonButton}}, Path=Name}" />
                            </r:RibbonButton.InputBindings>
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnCanales" 
                                        Label="{Binding lrbtnCanales, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png"  
                                        DataContext="{StaticResource mostrarAuxiliaresViewModel}" >
                            <r:RibbonButton.InputBindings>
                                <KeyBinding Key="Return"                                                
                                            Command="{Binding MostrarAuxCommand}" 
                                            CommandParameter="{Binding RelativeSource={RelativeSource AncestorType={x:Type r:RibbonButton}}, Path=Name}" />
                                <MouseBinding MouseAction="LeftClick"
                                              Command="{Binding MostrarAuxCommand}" 
                                              CommandParameter="{Binding RelativeSource={RelativeSource AncestorType={x:Type r:RibbonButton}}, Path=Name}" />
                            </r:RibbonButton.InputBindings>
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnCargosPersonal" 
                                        Label="{Binding lrbtnCargosPersonal, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" 
                                        DataContext="{StaticResource mostrarAuxiliaresViewModel}" >
                            <r:RibbonButton.InputBindings>
                                <KeyBinding Key="Return"                                                
                                            Command="{Binding MostrarAuxCommand}" 
                                            CommandParameter="{Binding RelativeSource={RelativeSource AncestorType={x:Type r:RibbonButton}}, Path=Name}" />
                                <MouseBinding MouseAction="LeftClick"
                                              Command="{Binding MostrarAuxCommand}" 
                                              CommandParameter="{Binding RelativeSource={RelativeSource AncestorType={x:Type r:RibbonButton}}, Path=Name}" />
                            </r:RibbonButton.InputBindings>
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnClavesPresupuesto" 
                                        Label="{Binding lrbtnClavesPresupuesto, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnFormasCobroClientes" 
                                        Label="{Binding lrbtnFormasCobroClientes, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnMercados" 
                                        Label="{Binding lrbtnMercados, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnNegocios" 
                                        Label="{Binding lrbtnNegocios, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnOrigenClientes" 
                                        Label="{Binding lrbtnOrigenClientes, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnPaises" 
                                        Label="{Binding lrbtnPaises, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnPoblaciones" 
                                        Label="{Binding lrbtnPoblaciones, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnProvincias" 
                                        Label="{Binding lrbtnProvincias, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnSectoresActividad" 
                                        Label="{Binding lrbtnSectoresActividad, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnTarjetasCredito" 
                                        Label="{Binding lrbtnTarjetasCredito, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnTarjetasEmpresa" 
                                        Label="{Binding lrbtnTarjetasEmpresa, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnTiposCliente" 
                                        Label="{Binding lrbtnTiposCliente, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnTiposContacto" 
                                        Label="{Binding lrbtnTiposContacto, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnTiposVisita" 
                                        Label="{Binding lrbtnTiposVisita, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnUsoAlquiler" 
                                        Label="{Binding lrbtnUsoAlquiler, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnVendedores" 
                                        Label="{Binding lrbtnVendedores, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                        <r:RibbonButton x:Name="rbtnZonas" 
                                        Label="{Binding lrbtnZonas, Source={StaticResource ResourceLanguage}}" 
                                        HorizontalAlignment="Left" 
                                        SmallImageSource="..\Images\new.png" >
                        </r:RibbonButton>
                    </r:RibbonMenuButton>
                    <r:RibbonMenuButton x:Name="rmnbtnAuxiliaresComisionistas" Label="{Binding lrmnbtnAuxiliaresComisionistas, Source={StaticResource ResourceLanguage}}" LargeImageSource="..\Images\open.png" KeyTip="M" >
                        <r:RibbonButton x:Name="rbtnEmpleadosAgencia"
                                            Label="{Binding lrbtnEmpleadosAgencia, Source={StaticResource ResourceLanguage}}" 
                                            HorizontalAlignment="Left" 
                                            SmallImageSource="..\Images\new.png" />
                        <r:RibbonButton x:Name="rbtnTipoComisionista"                                            
