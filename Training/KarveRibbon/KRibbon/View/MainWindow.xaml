<r:RibbonWindow x:Class="KRibbon.MainWindow"    
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                xmlns:r="clr-namespace:Microsoft.Windows.Controls.Ribbon;assembly=RibbonControlsLibrary"
                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"                
                xmlns:local="clr-namespace:KRibbon"
                xmlns:vm="clr-namespace:KRibbon.ViewModel"
                mc:Ignorable="d"
                Title="MainWindow" >
    <r:RibbonWindow.DataContext>
        <vm:SetLanguagesViewModel/>
    </r:RibbonWindow.DataContext>
    <DockPanel x:Name="RibbonWindow">
        <!--StatusBar-->
        <StatusBar DockPanel.Dock="Bottom" x:Name="stbrInicio">
            <StatusBarItem>
                <TextBlock Name="stInicio" />
            </StatusBarItem>
        </StatusBar>
        <!--Permite hacer scroll entre las diferentes tabs de la ribbon-->
        <!--<ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Hidden">-->
        <r:Ribbon x:Name="rbInicio" DockPanel.Dock="Top" Title="Aquí ponemos algún texto, p.e.: KarveWin [Versión: 01/01/2000 00:00]         C1[PRUEBA, S.A]. USUARIO: JORDI">
            <r:Ribbon.Resources>
                <!--Resource que permite hacer drag&drop entre los ribbongroups-->
                <Style TargetType="r:RibbonGroup">
                    <Setter Property="AllowDrop" Value="True"/>
                    <EventSetter Event="PreviewMouseMove" Handler="RibbonGroup_PreviewMouseMove"/>
                    <EventSetter Event="Drop" Handler="RibbonGroup_Drop"/>
                </Style>

                <!--Resource que añade un ContextMenu personalizado a los items que deseemos-->
                <!--<r:RibbonContextMenu x:Key="AnadirAFavoritos" x:Name="rctmnAnadir">
                        <r:RibbonMenuItem Header="Añadir a Favoritos" x:Name="rctmnitSalir" >
                            <r:RibbonMenuItem.Icon>
                                <Image Source="..\Images\new.png" />
                            </r:RibbonMenuItem.Icon>
                        </r:RibbonMenuItem>
                        <Separator/>
                        <r:RibbonMenuItem Header="Item1" >
                            <r:RibbonMenuItem.Icon>
                                <Image Source="..\Images\cut.png" />
                            </r:RibbonMenuItem.Icon>
                        </r:RibbonMenuItem>
                        <r:RibbonMenuItem Header="Item2" Command="Copy">
                            <r:RibbonMenuItem.Icon>
                                <Image Source="..\Images\copy.png" />
                            </r:RibbonMenuItem.Icon>
                        </r:RibbonMenuItem>
                        <r:RibbonMenuItem Header="Item3" Command="Paste">
                            <r:RibbonMenuItem.Icon>
                                <Image Source="..\Images\paste.png" />
                            </r:RibbonMenuItem.Icon>
                        </r:RibbonMenuItem>
                    </r:RibbonContextMenu>-->
                <!--Resource que da formato a los RibbonGroup cuando se minimiza/collapse la pantalla-->
                <r:RibbonGroupSizeDefinitionBaseCollection x:Key="RibbonGroupLayoutLarge">
                    <r:RibbonGroupSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Large" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Large" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Large" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Large" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Large" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Large" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Large" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Large" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                    </r:RibbonGroupSizeDefinition>
                    <r:RibbonGroupSizeDefinition IsCollapsed="True"></r:RibbonGroupSizeDefinition>
                </r:RibbonGroupSizeDefinitionBaseCollection>
                <r:RibbonGroupSizeDefinitionBaseCollection x:Key="RibbonGroupLayoutSmall">
                    <r:RibbonGroupSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Small" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Small" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Small" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Small" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Small" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Small" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                        <r:RibbonControlSizeDefinition ImageSize="Small" IsLabelVisible="True"></r:RibbonControlSizeDefinition>
                    </r:RibbonGroupSizeDefinition>
                    <r:RibbonGroupSizeDefinition IsCollapsed="True"></r:RibbonGroupSizeDefinition>
                </r:RibbonGroupSizeDefinitionBaseCollection>
            </r:Ribbon.Resources>

            <!--ContextualTabGroups-->
            <r:Ribbon.ContextualTabGroups>
                <r:RibbonContextualTabGroup Header="Karve" Visibility="Visible" Background="Coral" />
                <r:RibbonContextualTabGroup Header="Acciones" Visibility="Visible" Background="Azure" />
            </r:Ribbon.ContextualTabGroups>

            <!--QuickAccessToolBar-->
            <r:Ribbon.QuickAccessToolBar>
                <r:RibbonQuickAccessToolBar Visibility="Visible" >
                    <r:RibbonButton x:Name="rbtttCortar" 
                                    Label="Cortar" 
                                    SmallImageSource="..\Images\copy.png" 
                                    Command="Cut" />
                    <r:RibbonButton x:Name="rbtttCopiar" 
                                    Label="Copiar" 
                                    SmallImageSource="..\Images\copy.png" 
                                    Command="Copy" />
                    <r:RibbonButton x:Name="rbtttPegar" 
                                    Label="Pegar" 
                                    SmallImageSource="..\Images\paste.png" 
                                    Command="Paste" />
                    <r:RibbonButton x:Name="rbttEliminar" 
                                    Label="Eliminar" 
                                    SmallImageSource="..\Images\delete.png" 
                                    Command="Delete" />
                    <r:RibbonSplitButton x:Name="rbttRehacer" 
                                         Label="Rehacer" 
                                         SmallImageSource="..\Images\redo.png" 
                                         Command="Redo" >
                        <r:RibbonSplitMenuItem Header="Redo 1" />
                        <r:RibbonSplitMenuItem Header="Redo 2" />
                    </r:RibbonSplitButton>
                    <r:RibbonSplitButton x:Name="rbtttDeshacer" 
                                         Label="Deshacer" 
                                         SmallImageSource="..\Images\undo.png" 
                                         Command="Undo" >
                        <r:RibbonSplitMenuItem Header="Undo 1" />
                        <r:RibbonSplitMenuItem Header="Undo 2" />
                        <r:RibbonSplitMenuItem Header="Undo 3" />
                    </r:RibbonSplitButton>
                    <r:RibbonButton x:Name="rbtttOpciones" 
                                    Label="Opciones" 
                                    SmallImageSource="..\Images\tools.png" />
                </r:RibbonQuickAccessToolBar>
            </r:Ribbon.QuickAccessToolBar>

            <!--AplicationMenu-->
            <r:Ribbon.ApplicationMenu >
                <r:RibbonApplicationMenu x:Name="rapmnInicio"  SmallImageSource="..\Images\home.png" Label="{Binding lrapmnInicio, Source={StaticResource ResourceLanguage}}" KeyTip="H">
                    <r:RibbonApplicationMenuItem x:Name="rapmnitAbrir" 
                                                 Header="{Binding lrapmnitAbrir, Source={StaticResource ResourceLanguage}}"
                                                 ImageSource="..\Images\open.png" 
                                                 Command="Open" />
                    <r:RibbonApplicationMenuItem x:Name="rapmnitGuardar" 
                                                 Header="{Binding lrapmnitGuardar, Source={StaticResource ResourceLanguage}}" 
                                                 ImageSource="..\Images\save.png" 
                                                 Command="Save" />
                    <r:RibbonSeparator />
                    <r:RibbonApplicationMenuItem x:Name="rapmnitRelogin" 
                                                 Header="{Binding lrapmnitRelogin, Source={StaticResource ResourceLanguage}}" 
                                                 ImageSource="..\Images\login.png" />
                    <r:RibbonApplicationMenuItem x:Name="rapmnitReconectar" 
                                                 Header="{Binding lrapmnitReconectar, Source={StaticResource ResourceLanguage}}" 
                                                 ImageSource="..\Images\conect.png" />
                    <r:RibbonSeparator />
                    <r:RibbonApplicationMenuItem x:Name="rapmnitIrAlquiler" 
                                                 Header="{Binding lrapmnitIrAlquiler, Source={StaticResource ResourceLanguage}}"
                                                 ImageSource="..\Images\redirect.png" />
                    <r:RibbonApplicationMenuItem x:Name="rapmnitIrContabilidad" 
                                                 Header="{Binding lrapmnitIrContabilidad, Source={StaticResource ResourceLanguage}}" 
                                                 ImageSource="..\Images\redirect.png" />
                    <r:RibbonApplicationMenuItem x:Name="rapmnitIrTaller" 
                                                 Header="{Binding lrapmnitIrTaller, Source={StaticResource ResourceLanguage}}"
                                                 ImageSource="..\Images\redirect.png" />
                    <r:RibbonSeparator />
                    <r:RibbonApplicationMenuItem x:Name="rapmnitSalir"
                                                 Header="{Binding lrapmnitSalir, Source={StaticResource ResourceLanguage}}"
                                                 ImageSource="..\Images\exit.png"                                     
                                                 DataContext="{StaticResource closeItem}" >
                        <r:RibbonApplicationMenuItem.InputBindings>
                            <KeyBinding Key="Return"                                                
                                        Command="{Binding CloseItemCommand}" 
                                        CommandParameter="Window" />
                            <MouseBinding MouseAction="LeftClick"
                                          Command="{Binding CloseItemCommand}" 
                                          CommandParameter="Window" />
                        </r:RibbonApplicationMenuItem.InputBindings>
                    </r:RibbonApplicationMenuItem>
                </r:RibbonApplicationMenu>
            </r:Ribbon.ApplicationMenu>

            <!--HelpPaneContent-->
            <r:Ribbon.HelpPaneContent>
                <r:RibbonButton x:Name="rbttHelp" 
                                SmallImageSource="..\Images\help.png" 
                                Click="bttHelp_Click" 
                                ToolTipTitle="{Binding lrbttHelpTitle, Source={StaticResource ResourceLanguage}}"
                                ToolTipDescription="{Binding lrbttHelpDescr, Source={StaticResource ResourceLanguage}}"
                                ToolTipFooterTitle="{Binding lrbttHelpFooterTitle, Source={StaticResource ResourceLanguage}}" 
                                ToolTipFooterDescription="{Binding lrbttHelpFooterDescr, Source={StaticResource ResourceLanguage}}" 
                                ToolTipService.ShowDuration="10000"/>
            </r:Ribbon.HelpPaneContent>

            <!--Tab Maestros-->
            <r:RibbonTab x:Name="tbMaestros"
                         Header="{Binding ltbMaestros, Source={StaticResource ResourceLanguage}}"
                         KeyTip="M"
