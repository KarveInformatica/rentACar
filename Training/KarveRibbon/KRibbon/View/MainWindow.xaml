        <!--Toolbar Acciones-->
        <ToolBarTray x:Name="tbtAcciones" DockPanel.Dock="Top" >
            <ToolBar x:Name="tbAcciones" HorizontalAlignment="Left" ToolBar.OverflowMode="Never" >
                <Button x:Name="tbbttNuevo" 
                        HorizontalAlignment="Left"                             
                        DataContext="{StaticResource mostrarAuxiliares}" >
                    <StackPanel>
                        <Image Source="..\Images\new.png" Stretch="Fill" Height="35" />
                        <TextBlock Text="{Binding ltbbttNuevo, Source={StaticResource ResourceLanguage}}" HorizontalAlignment="Center"/>
                    </StackPanel>
                    <Button.ToolTip>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="{Binding ltbbttNuevo, Source={StaticResource ResourceLanguage}}" />
                            <TextBlock Text=" (Ctrl+N)" />
                        </StackPanel>
                    </Button.ToolTip>
                    <Button.InputBindings>
                        <KeyBinding Key="N" 
                                    Modifiers="Ctrl" 
                                    Command="{Binding NosequeCommand}" 
                                    CommandParameter="{Binding RelativeSource={RelativeSource AncestorType={x:Type r:RibbonButton}}, Path=Name}" />
                        <KeyBinding Key="Return"                                                
                                    Command="{Binding NosequeCommand}" 
                                    CommandParameter="{Binding RelativeSource={RelativeSource AncestorType={x:Type r:RibbonButton}}, Path=Name}" />
                        <MouseBinding MouseAction="LeftClick"
                                        Command="{Binding NosequeCommand}" 
                                        CommandParameter="{Binding RelativeSource={RelativeSource AncestorType={x:Type r:RibbonButton}}, Path=Name}" />
                        
                    </Button.InputBindings>
                </Button>
                <Button x:Name="tbbttEditar" 
                        HorizontalAlignment="Left"                             
                        DataContext="{StaticResource mostrarAuxiliares}" >
                    <StackPanel>
                        <Image Source="..\Images\edit.png" Stretch="Fill" Height="35" />
                        <TextBlock Text="{Binding ltbbttEditar, Source={StaticResource ResourceLanguage}}" HorizontalAlignment="Center"/>
                    </StackPanel>
                    <Button.ToolTip>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="{Binding ltbbttEditar, Source={StaticResource ResourceLanguage}}" />
                            <TextBlock Text=" (Ctrl+E)" />
                        </StackPanel>
                    </Button.ToolTip>
                    <Button.InputBindings>
                        <KeyBinding Key="E" 
                                    Modifiers="Ctrl" 
                                    Command="{Binding NosequeCommand}" 
                                    CommandParameter="{Binding RelativeSource={RelativeSource AncestorType={x:Type r:RibbonButton}}, Path=Name}" />
